#: ../src/music-scope.cpp:122
msgid "Songs"
msgstr "Abestiak"

#: ../src/music-scope.cpp:159
msgid "Albums"
msgstr "Albumak"

#: ../src/music-scope.cpp:241 ../src/music-scope.cpp:278
msgid "Play in music app"
msgstr "Erreproduzitu musika-aplikazioan"

#: ../src/video-scope.cpp:102
